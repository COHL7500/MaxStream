<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.FlowPane?>

<FlowPane alignment="CENTER" xmlns:fx="http://javafx.com/fxml"
          fx:controller="project_code.ProfileController" vgap="100" hgap="10">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="60.0"/>
    </padding>

    <Label fx:id="welcomeText2"/>
    <Button text="1" onAction="#onAnotherButtonClick"/>
    <Button text="2" onAction="#onAnotherButtonClick"/>
    <Button text="3" onAction="#onAnotherButtonClick"/>

    <HBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="20.0"/>
        </padding>

        <Label fx:id="welcomeText" layoutX="100" layoutY="300"/>
        <Button text="Hello!" onAction="#onHelloButtonClick"/>
        <Button text="spaghetti" onAction="#onHelloButtonClick"/>
        <Button text="kokos" onAction="#onHelloButtonClick"/>
    </HBox>
</FlowPane>




